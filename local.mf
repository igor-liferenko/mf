% NOTE: if this file must be different on different computers, make it a symlink to /usr/local/
% NOTE: if you change something here, `rm ~/tex/TeXfonts/*pk' and `make'
% TODO: set resolution to 300
mode_def localfont =
  pixels_per_inch := 600; % if this is changed, gray.tfm and black.tfm must be regenerated
  blacker := .25;
  o_correction := 1;
  fillin := 0;
  proofing := 0;
  fontmaking := 1;
  tracingtitles := 1;
enddef;
